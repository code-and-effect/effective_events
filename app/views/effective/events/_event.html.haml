.effective-event
  %table.table.effective-event-table
    %tbody
      %tr
        %td Starts
        %td= event.start_at

      %tr
        %td Ends
        %td= event.end_at

      %tr
        %td Registration Closes
        %td= event.registration_end_at

      %tr
        %td Early Bird Status
        %td= event.early_bird_status

  .event-content.event-excerpt
    = event.excerpt
