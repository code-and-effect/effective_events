= effective_form_with(model: [:admin, event], engine: true) do |f|
  = f.text_field :title
  = f.rich_text_area :body

  = f.datetime_field :start_at
  = f.datetime_field :end_at
  = f.datetime_field :registration_start_at
  = f.datetime_field :registration_end_at
  = f.datetime_field :early_bird_end_at

  = effective_submit(f)
